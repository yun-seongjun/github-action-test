{
  "name": "@neubie/neubie-order-admin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "web": "nx dev",
    "dev": "node remove-jsx.js && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen-dev": "rm -rf src/api/generated && orval --config orval.dev.config.js",
    "gen-staging": "rm -rf src/api/generated && orval --config orval.staging.config.js",
    "build-icons": "node ../../scripts/build-icon-script.js && node ../../scripts/lazy-icon-script.js",
    "prepare": "husky install && chmod +x .husky/pre-commit && chmod +x .husky/pre-push",
    "type-inspect": "tsc --noEmit --incremental -p tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nx/next": "^20.3.0",
    "daisyui": "^2.52.0",
    "i18next": "23.5.1",
    "next": "14.2.16",
    "next-i18next": "^14.0.3",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "18.2.0",
    "react-i18next": "13.2.2",
    "react-tooltip": "^5.28.0"
  },
  "nx": {
    "name": "neubie-order-admin",
    "projectType": "application",
    "sourceRoot": "apps/neubie-order-admin"
  },
  "devDependencies": {
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/crypto-js": "^4.2.2",
    "@types/geojson": "^7946.0.15",
    "@types/navermaps": "^3.7.8"
  }
}
